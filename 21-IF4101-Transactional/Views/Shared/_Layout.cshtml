    <!-- REGISTER APPLICANT -->
    <section id="addApplicant" class="displayNoUser">
        <div class="container">
            <div class="row">

                <div class="col-md-6 col-sm-12">
                    <div class="addApplicant-info">
                        <h2>Únase a la comunidad de informática empresarial</h2>
                        <figure>
                            <span><i class="fa fa-newspaper-o"></i></span>
                            <figcaption>
                                <h3>Nuevas noticias</h3>
                                <p>Conozca y comenta las novedades de Informática Empresarial.</p>
                            </figcaption>
                        </figure>

                        <figure>
                            <span><i class="fa fa-question"></i></span>
                            <figcaption>
                                <h3>Consultas a la comunidad</h3>
                                <p>Haga y responda preguntas a los estudiantes de la comunidad.</p>
                            </figcaption>
                        </figure>

                        <figure>
                            <span><i class="fa fa-calendar"></i></span>
                            <figcaption>
                                <h3>Solicitar cita</h3>
                                <p>Solicite su cita con un profesor de la comunidad.</p>
                            </figcaption>
                        </figure>

                    </div>
                </div>

                <div class="col-md-4 col-sm-12">
                    <div class="entry-form">
                        <form id="registerApplicantForm" autocomplete="off">
                            <h2>Únete a la comunidad</h2>
                            <input type="text" name="firstNameApplicant" id="firstNameApplicant" class="form-control" placeholder="Nombre" required="" minlength="3" maxlength="50" pattern="[A-Za-z áéíóúñÑ]+">

                            <input type="text" name="lastNameApplicant" id="lastNameApplicant" class="form-control" placeholder="Apellidos" required="" minlength="10" maxlength="50" pattern="[A-Za-záéíóúñÑ](?=.* ).{1,}[A-Za-záéíóúñÑ]+" title="Inserte los dos apellidos">

                            <input type="text" name="studentIdApplicant" id="studentIdApplicant" class="form-control" placeholder="Carnet" required="" minlength="6" maxlength="6" pattern="(?=.*[A-Z]).{1}[0-9]+" title="Valide su formato de Carnet">

                            <input type="email" name="emailApplicant" id="emailApplicant" class="form-control" placeholder="Correo Institucional" required="" pattern="[\w.%+-]+@@ucr\.ac\.cr" title="El correo debe llevar el dominio @@ucr.ac.cr">

                            <input type="password" name="passwordApplicant" id="passwordApplicant" class="form-control" placeholder="Contraseña" required="" minlength="8" maxlength="8" pattern="^(?=.*[a-z])(?=.*[A-Z])(?=.*[0-9])(?=.*[!.#$%^&*_=+-]).{8,8}$" title="La contraseña necesita al menos 1 mayúscula, 1 minúscula, 1 número y un 1 símbolo (!. # $% ^ & * _ = + -)">
                            <span id="showPasswordApplicant"><i class="fa fa-eye" id="iconPasswordApplicant"></i></span>

                            <button type="submit" class="submit-btn form-control">Enviar</button>

                            <div id="alertMessageAddStudent"></div>
                        </form>
                    </div>
                </div>

            </div>
        </div>
    </section>


