                        <div class="table-responsive">
                            <table class="table table-striped table-bordered" id="professorTable">
                                <thead class="table-primary headTable">
                                    <tr class="text-center">
                                        <th>Id</th>
                                        <th>Nombre</th>
                                        <th>Correo Electrónico</th>
