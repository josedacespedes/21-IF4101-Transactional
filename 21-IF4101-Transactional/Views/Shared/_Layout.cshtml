    <!--  REGISTER COURSE -->
    <section id="addCourse" class="displayAdmin">
        <div class="container">
            <div class="row">

                <!-- ADD  -->
                <div class="col-md-6 col-sm-12">
                    <div class="entry-form">
                        <form id="registerCourseForm" autocomplete="off">
                            <h2>Agregar Curso</h2>
                            <input type="text" name="courseName" id="courseName" class="form-control" placeholder="Nombre" required="" minlength="10" maxlength="150" title="Complete este campo con el nombre del curso">

                            <input type="text" name="idCourse" id="idCourse" class="form-control" placeholder="Código" required="" minlength="4" maxlength="4" pattern="[0-9IF-]+" title="Complete este campo con 4 números">

                            <input type="text" name="creditsNumber" id="creditsNumber" class="form-control" placeholder="Créditos" required="" minlength="1" maxlength="1" pattern="[1-4]+" title="Complete esta campo con un número en el rango de 1 a 4">

                            <div class="switchFlex">
                                <label id="courseStatusLabelOne">Estado</label>
                                <label class="switch" id="courseStatusLabelTwo">
                                    <input type="checkbox" name="courseStatus" id="courseStatus" title="Escoja si el Curso esta activo o no">
