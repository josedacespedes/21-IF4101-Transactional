    <!--  MODAL COURSE-GROUP -->
    <div class="modal fade" id="modalCourseGroup" role="dialog">
        <div class="modal-dialog">

            <!-- Modal content-->
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" id="buttonCloseModal" class="btn btn-danger" data-dismiss="modal" onclick="cleanFieldsModalCourseGroup()" aria-label="Close"><b>X</b></button>
                    <h2 class="modal-title">Asociar Curso-Grupo</h2>
                </div>
                <div class="modal-body">
                    <div id="courseTitleModal"></div>

                    <form id="registerCourseGroup" autocomplete="off">
                        <div class="form-group" id="inputAmount">
                            <label for="usr">Cantidad de grupos:</label>
                            <div id="inputAmountPosition">
                                <input type="number" class="form-control" id="groupsAmount" name="groupsAmount" onkeyup="calculateGroupsAmount()" placeholder="Ingrese solo números" onkeydown="javascript: return event.keyCode == 69 ? false : true" required="" min="1" max="99" title="Sólo permite números enteros positivos." />
                            </div>
                        </div>
                        <div class="form-group" id="inputAssociate">
                            <label for="pwd">Primer Grupo:</label>
                            <div id="inputAssociatePosition">
                                <input type="number" id="associateGroupCourse" name="associateGroupCourse" placeholder="Ingrese solo números" onkeyup="calculateGroupsAmount()" onkeydown="javascript: return event.keyCode == 69 ? false : true" class="form-control" min="1" max="99" title="Sólo permite números enteros positivos." required="" />
                            </div>
                        </div>
                        <div class="form-group" id="textAreaCourseGroup">
                            <label for="comment">Lista</label>
                            <textarea class="form-control" rows="5" id="showAssociation" placeholder="Cursos agregados: " disabled></textarea>
                        </div>

                        <div class="modal-footer mt-4">
                            <div class="col-12">
                                <button type="submit" id="buttonAddCourseGroup" class="btn btn-success">Aceptar</button>
                                <button type="button" class="btn btn-danger" onclick="cleanFieldsModalCourseGroup()" data-dismiss="modal">Cerrar</button>
                            </div>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>

